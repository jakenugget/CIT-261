<!DOCTYPE HTML>
<html lang="en-us">
<head>
<meta charset="utf-8">
<title>Title</title>

<script>
/* Input: User Number
* Processing: If greater than 15
* Output: True or False
*/
function fifteen() {
var num1 = parseInt(document.getElementById('firstInputBox').value);
var num2 = parseInt(document.getElementById('secondInputBox').value);
var output = "False"

if (num1 > 15 && num2 > 15) 
{
output = "True";
}
else if (num1 + num2 > 15)
{
output = "True";
}
else
{
output = "False";
}
document.getElementById('fifteenDiv').innerHTML = output;
}
</script>
</head>

<body>
What is the first number : <input type="text" id="firstInputBox">
What is the second number : <input type="text" id="secondInputBox">
<button type="button" onclick="fifteen()">Fifteen</button>
<div id="fifteenDiv"></div>
</body>
</html>
