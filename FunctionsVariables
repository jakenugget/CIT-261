<!DOCTYPE HTML>
<html lang="en-us">
<head>
<meta charset="utf-8">
<title>Calculate 1000 Pound Club</title>

<script>
/* Input: Bench, Squat and Deadlift
 * Processing: Add the Numbers
 * Output: Sum of Numbers
 */
function swolelit() {
    // Read a measurement.
    var text1 = document.getElementById('bench1InputBox').value;
    var text2 = document.getElementById('squat2InputBox').value;
    var text3 = document.getElementById('dead3InputBox').value;

    // Convert what the user typed from text into a number.
    var w = parseFloat(text1);
    var l = parseFloat(text2);
    var z = parseFloat(text3);

    // Convert the measurement to acres.
    var a = w + l + z;

    // Display the Acres to the user.
    document.getElementById('swoleDiv').innerHTML = a + " Pounds " ;
}
</script>
</head>

<body>                               
Enter your Bench: <input type="text1" id="bench1InputBox">
Enter your Squat: <input type="text2" id="squat2InputBox">
Enter your Deadlift: <input type="text3" id="dead3InputBox">
<button type="button" onclick="swolelit()">Convert</button>
<div id="swoleDiv"></div>                     
</body>            
</html>
